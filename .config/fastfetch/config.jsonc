{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "bar": {
      "char": {
        "elapsed": "=",
      },
      "charTotal": "-",
      "width": 13
    },
    "key": {
      "type": "icon",
    },
    "percent": {
      "type": 2
    }
  },
  "modules": [
    {
      "type": "title",
      "color": {
        "user": "green",
        "at": "red",
        "host": "blue"
      }
    },
    "separator",
    {
      "type": "os",
      "keyColor": "cyan"
    },
    {
      "type": "kernel",
      "keyColor": "cyan"
    },
    {
      "type": "uptime",
      "keyColor": "cyan"
    },
    {
      "type": "packages",
      "keyColor": "cyan"
    },
    {
      "type": "localip",
      "keyColor": "cyan"
    },
    {
      "type": "locale",
      "keyColor": "cyan"
    },
    "break",

    {
      "type": "host",
      "keyColor": "magenta"
    },
    {
      "type": "cpu",
      "keyColor": "magenta"
    },
    {
      "type": "gpu",
      "keyColor": "magenta"
    },
    {
      "type": "memory",
      "keyColor": "magenta"
    },
    {
      "type": "swap",
      "keyColor": "magenta"
    },
    {
      "type": "disk",
      "keyColor": "magenta",
    },
    {
