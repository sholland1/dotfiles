{
    "keyboard.dispatch": "keyCode",
    "vim.leader": "<space>",
    "vim.foldfix": true,
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                ";"
            ],
            "after": [
                "g",
                "c"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "p"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                ":w"
            ]
        },
        {
            "before": [
                "Y"
            ],
            "after": [
                "y",
                "$"
            ]
        },
        {
            "before": [
                "<cr>"
            ],
            "after": [
                "o",
                "<Esc>"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "commands": [
                ":q"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                ":e ~/.config/Code/User/settings.json"
            ]
        },
        {
            "before": [
                "[",
                "e"
            ],
            "commands": [
                "editor.action.marker.prevInFiles"
            ]
        },
        {
            "before": [
                "]",
                "e"
            ],
            "commands": [
                "editor.action.marker.nextInFiles"
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "commands": [
                "redo"
            ]
        },
        {
            "before": [
                "<leader>",
                ";"
            ],
            "after": [
                "g",
                "c"
            ]
        },
        {
            "before": [
                "<leader>",
                ";",
                ";"
            ],
            "after": [
                "g",
                "c",
                "c"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        }
    ],
    "workbench.colorTheme": "Wal Bordered",
    "window.menuBarVisibility": "toggle",
    "workbench.iconTheme": "vscode-icons",
    "vim.handleKeys": {
        "<C-k>": false,
        "<C-d>": true,
        "<C-p>": false,
    },
    "debug.onTaskErrors": "showErrors",
    "editor.lineHeight": 13,
    "editor.letterSpacing": -0.4,
    "editor.matchBrackets": "near",
    "editor.renderLineHighlight": "all",
    "editor.roundedSelection": false,
    "update.mode": "none",
    "explorer.confirmDelete": false,
    "vsicons.dontShowNewVersionMessage": true,
    "explorer.sortOrderLexicographicOptions": "lower",
    "editor.formatOnSaveMode": "modifications",
    "editor.formatOnType": true,
    "telemetry.telemetryLevel": "off",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
    "C_Cpp.clang_format_style": "{BasedOnStyle: LLVM, BreakBeforeBraces: Attach, IndentWidth: 4, TabWidth: 4, UseTab: Never, ColumnLimit: 0}",
    "workbench.editorAssociations": {
        "*.eep": "hexEditor.hexedit",
        "*.sra": "hexEditor.hexedit",
        "*.fla": "hexEditor.hexedit"
    },
    "omnisharp.useEditorFormattingSettings": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDragAndDrop": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "makefile.configureOnOpen": true,
    "files.insertFinalNewline": true,
    "[c]": {
        "files.trimTrailingWhitespace": true,
    },
    "[csharp]": {
        "files.trimTrailingWhitespace": true,
    },
    "editor.parameterHints.enabled": true,
    "C_Cpp.enhancedColorization": "enabled",
    "C_Cpp.intelliSenseEngine": "default",
    "editor.inlayHints.enabled": "on",
    "C_Cpp.inlayHints.parameterNames.enabled": true,
    "C_Cpp.inlayHints.parameterNames.suppressWhenArgumentContainsName": true,
    "debug.console.closeOnEnd": true,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "cSpell.userWords": [
        "Deadlift",
        "effectful",
        "Functors",
        "Kinded",
        "LINQ",
        "pickleball",
        "skybox",
        "typeclass",
        "typeclasses",
        "unmutes",
        "unmuting"
    ],
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "zig.zls.enabled": "on",
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "vim.useSystemClipboard": true,
    "python.analysis.typeCheckingMode": "standard",
    "editor.mouseWheelScrollSensitivity": 3,
    "javascript.format.semicolons": "insert",
    "typescript.format.semicolons": "insert",
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "search.seedWithNearestWord": true,
    "search.searchEditor.focusResultsOnSearch": true,
}
