{
    "keyboard.dispatch": "keyCode",
    "vim.leader": "<space>",
    "vim.foldfix": true,
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                ";"
            ],
            "after": [
                "g",
                "c"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "p"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                ":w"
            ]
        },
        {
            "before": [
                "Y"
            ],
            "after": [
                "y",
                "$"
            ]
        },
        {
            "before": [
                "<cr>"
            ],
            "after": [
                "o",
                "<Esc>"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "commands": [
                ":q"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                ":e ~/.config/Code/User/settings.json"
            ]
        },
        {
            "before": [
                "[",
                "e"
            ],
            "commands": [
                "editor.action.marker.prevInFiles"
            ]
        },
        {
            "before": [
                "]",
                "e"
            ],
            "commands": [
                "editor.action.marker.nextInFiles"
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "commands": [
                "redo"
            ]
        },
        {
            "before": [
                "<leader>",
                ";"
            ],
            "after": [
                "g",
                "c"
            ]
        },
        {
            "before": [
                "<leader>",
                ";",
                ";"
            ],
            "after": [
                "g",
                "c",
                "c"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        }
    ],
    "workbench.colorTheme": "Ayu Mirage Bordered",
    "window.menuBarVisibility": "toggle",
    "workbench.iconTheme": "vscode-icons",
    "vim.handleKeys": {
        "<C-k>": false,
        "<C-d>": true,
        "<C-p>": false,
    },
    "debug.onTaskErrors": "showErrors",
    "editor.lineHeight": 13,
    "editor.letterSpacing": -0.4,
    "editor.matchBrackets": "near",
    "editor.renderLineHighlight": "all",
    "editor.roundedSelection": false,
    "update.mode": "none",
    "explorer.confirmDelete": false,
    "vsicons.dontShowNewVersionMessage": true,
    "explorer.sortOrderLexicographicOptions": "lower",
    "editor.formatOnSaveMode": "modifications",
    "editor.formatOnType": true,
    "telemetry.telemetryLevel": "off",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false,
        "yaml": false,
        "zig": true,
        "rust": true,
        "hare": false
    },
    "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
    "zig.zls.path": "/home/sholland/.config/Code/User/globalStorage/ziglang.vscode-zig/zls_install/zls",
    "workbench.editorAssociations": {
        "*.eep": "hexEditor.hexedit",
        "*.sra": "hexEditor.hexedit",
        "*.fla": "hexEditor.hexedit"
    },
    "omnisharp.useEditorFormattingSettings": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDragAndDrop": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
}
