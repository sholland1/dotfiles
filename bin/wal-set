#!/bin/bash
echo "$1" > ~/.cache/.bg
# nvim
~/.config/nvim/reload_neovim.py &
# dwm
xdotool key Super+Shift+Alt+Control+F12 &
# qutebrowser
~/.config/qutebrowser/reload_colors.py &
# tig/lazygit
# sc-im
# zathura
# neomutt
# htop
# mgba
# alacritty
cat ~/.alacritty-rest.yml ~/.cache/wal/colors-alacritty.yml > ~/.alacritty.yml &

